{
  "name": "way-farer",
  "version": "1.0.0",
  "description": "Andela Developer Challenge",
  "private": true,
  "scripts": {
    "test": "mocha --reporter progress test",
    "webpack": "webpack",
    "start": "babel-node app/server.js",
    "prestart:dev": "npm-run-all --parallel security-check lint:watch",
